b.org/3/search/movie?api_key=${apiKey}&query=${searchValue}&page=1`\n    );\n    const movieDetail = await data.json();\n    setMoviedet(movieDetail.results);\n  };\n\n  useEffect(() => {\n    fetchMovies();\n  }, [searchValue]);\n\n  return (\n    <div>\n      {!searchValue ? (\n        \"\"\n      ) : (\n        <div>\n          <h2>Searched Movies</h2>\n        </div>\n      )}\n      <div>\n        {moviedet.map((movie) => (\n          <div key={movie.id}>\n            <div>\n              <a href={`/movie/${movie.id}`}>\n                {movie.poster_path ? (\n                  <img\n                    className=\"image\"\n                    src={\n                      \"https://image.tmdb.org/t/p/original/\" + movie.poster_path\n                    }\n                    alt={movie.title}\n                  />\n                ) : (\n                  <img\n                    src=\"https://media.istockphoto.com/id/1208666888/vector/marquee-and-curtain-background.jpg?s=612x612&w=0&k=20&c=VyNG1C6kfoOoH3W7cNNMmyNAlAiLuuqoQWTdDLAvA14=\"\n                    alt={movie.title}\n                  />\n                )}\n              </a>\n              <p>{movie.title}</p>\n            </div>\n          </div>\n        ))}\n      </div>\n    </div>\n  );\n}\n","/Users/angel/Desktop/bootcamp/Project/project-3/client/src/components/Search.js",[],"/Users/angel/Desktop/bootcamp/Project/project-3/client/src/utils/mutations.js",[],{"ruleId":"114","severity":1,"message":"115","line":14,"column":22,"nodeType":"116","messageId":"117","endLine":14,"endColumn":27},{"ruleId":"114","severity":1,"message":"118","line":14,"column":29,"nodeType":"116","messageId":"117","endLine":14,"endColumn":33},{"ruleId":"114","severity":1,"message":"115","line":13,"column":19,"nodeType":"116","messageId":"117","endLine":13,"endColumn":24},{"ruleId":"114","severity":1,"message":"118","line":13,"column":26,"nodeType":"116","messageId":"117","endLine":13,"endColumn":30},{"ruleId":"119","severi