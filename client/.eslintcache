\";\n\nfunction Reviews() {\n  const [reviewData, setReviewData] = useState({\n    title: \"\",\n    body: \"\",\n    rating: 0\n  });\n\n  const [createReview, { error }] = useMutation(CREATE_REVIEW);\n\n  const handleChange = (event) => {\n    const { name, value } = event.target;\n    setReviewData({\n      ...reviewData,\n      [name]: value\n    });\n  };\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n    try {\n      await createReview({\n        variables: reviewData\n      });\n      setReviewData({\n        title: \"\",\n        body: \"\",\n        rating: 0\n      });\n    } catch (error) {\n      console.log(error);\n    }\n  };\n\n  return (\n    <div>\n      <h2>Review</h2>\n      <form onSubmit={handleFormSubmit}>\n        <div>\n          <label htmlFor=\"title\">Title:</label>\n          <input\n            type=\"text\"\n            id=\"title\"\n            name=\"title\"\n            value={reviewData.title}\n            onChange={handleChange}\n          />\n        </div>\n        <div>\n          <label htmlFor=\"body\">Body:</label>\n          <textarea\n            id=\"body\"\n            name=\"body\"\n            value={reviewData.body}\n            onChange={handleChange}\n          />\n        </div>\n        <div>\n          <label htmlFor=\"rating\">Rating:</label>\n          <input\n            type=\"number\"\n            id=\"rating\"\n            name=\"rating\"\n            value={reviewData.rating}\n            onChange={handleChange}\n            max={5}\n          />\n        </div>\n        <button type=\"submit\">Submit</button>\n      </form>\n    </div>\n  );\n}\n\nexport default Reviews;\n","C:\\Users\\Andy\\Desktop\\BootCamp\\project\\project-3\\client\\src\\pages\\Favorites\\Favorites.js",[],"C:\\Users\\Andy\\Desktop\\BootCamp\\project\\project-3\\client\\src\\pages\\watchList\\WatchList.js",[],"C:\\Users\\Andy\\Desktop\\BootCamp\\project\\project-3\\client\\src\\utils\\auth.js",["113"],"